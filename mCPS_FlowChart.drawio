        <mxCell id="pdw2pKIptuRdF_-jsix2-111" value="&lt;div&gt;&amp;nbsp;SELECT A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID, A.DEMAND_ORDER_NO, A.LVL, A.LVL_R, A.PLANT_CD, A.WC_CD, A.ORDER_TYPE&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.ITEM_CD, A.ITEM_TYPE, A.PARENT_ITEM_CD, A.CUST_CD, A.PLAN_STATUS, A.DUE_DATE, A.REF_ORDER_NO, A.SETUP_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY, A.TRANSFER_QTY, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.TRANSFER_MIN, A.BOD_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET, A.PRE_PST, A.RUN_MIN, A.BASE_RUN_MIN, A.WORK_MIN, A.PRIORITY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WORK_MIN &lt;/font&gt;&lt;/b&gt;/ (SELECT K.&lt;b&gt;&lt;font color=&quot;#ff8000&quot;&gt;AVG_WORK_MIN &lt;/font&gt;&lt;/b&gt;FROM AVG_WORK K WHERE A.PLANT_CD = K.PLANT_CD AND A.WC_CD = K.WC_CD) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN A.LVL_R = 1 THEN 0&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt; MAX(A.ACCUM_WORK_MIN) &lt;/font&gt;&lt;/b&gt;OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.DEMAND_ID) - &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.ACCUM_WORK_MIN&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; END AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRE_ACCUM_WC_WORK_MIN&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN A.LVL_R = 1 THEN A.WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE MAX(A.ACCUM_WORK_MIN) OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.DEMAND_ID) - A.ACCUM_WORK_MIN + A.WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; END AS ACCUM_WC_WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN A.LVL_R = 1 THEN &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;0&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX(A.ACCUM_WORK_BUCKET&lt;/font&gt;&lt;/b&gt;) OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.DEMAND_ID) - &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.ACCUM_WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; END AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRE_ACCUM_DMD_WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN A.LVL_R = 1 THEN &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.WORK_MIN &lt;/font&gt;&lt;/b&gt;/ (SELECT K.&lt;b&gt;&lt;font color=&quot;#ff8000&quot;&gt;AVG_WORK_MIN&lt;/font&gt;&lt;/b&gt; FROM AVG_WORK K WHERE A.PLANT_CD = K.PLANT_CD AND A.WC_CD = K.WC_CD)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX(A.ACCUM_WORK_BUCKET)&lt;/font&gt;&lt;/b&gt; OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.DEMAND_ID) - &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.ACCUM_WORK_BUCKET &lt;/font&gt;&lt;/b&gt;+ A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; END AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;ACCUM_DMD_WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM (&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;SELECT A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID, A.DEMAND_ORDER_NO, A.LVL, A.LVL_R, A.PLANT_CD, A.WC_CD, A.ORDER_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.ITEM_CD, A.ITEM_TYPE, A.PARENT_ITEM_CD, A.CUST_CD, A.PLAN_STATUS, A.DUE_DATE, A.REF_ORDER_NO, A.SETUP_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY, A.TRANSFER_QTY, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.TRANSFER_MIN, A.BOD_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET, A.PRE_PST, A.RUN_MIN, A.BASE_RUN_MIN, A.WORK_MIN, A.PRIORITY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PRE_ACCUM_WC_WORK_MIN, A.ACCUM_WC_WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , ROW_NUMBER() OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.DEMAND_ID, A.PROD_ORDER_NO ORDER BY A.LVL_R DESC) AS SEQ&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.WORK_MIN &lt;/font&gt;&lt;/b&gt;/ (SELECT K.&lt;b&gt;&lt;font color=&quot;#ff8000&quot;&gt;AVG_WORK_MIN &lt;/font&gt;&lt;/b&gt;FROM AVG_WORK K WHERE A.PLANT_CD = K.PLANT_CD AND A.WC_CD = K.WC_CD) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;SUM(B.WORK_MIN)&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.DEMAND_ID, A.PROD_ORDER_NO ORDER BY&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt; A.LVL_R DESC)&lt;/font&gt;&lt;/b&gt; AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;ACCUM_WORK_MIN&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;SUM(B.WORK_MIN &lt;/font&gt;&lt;/b&gt;/ (SELECT K.&lt;b&gt;&lt;font color=&quot;#ff8000&quot;&gt;AVG_WORK_MIN &lt;/font&gt;&lt;/b&gt;FROM AVG_WORK K WHERE B.PLANT_CD = K.PLANT_CD AND B.WC_CD = K.WC_CD))&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.DEMAND_ID, A.PROD_ORDER_NO ORDER BY &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.LVL_R DESC&lt;/font&gt;&lt;/b&gt;) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;ACCUM_WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM&amp;nbsp; MASTER_PLAN_WC A&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; INNER JOIN MASTER_PLAN_WC B&amp;nbsp; --이후 작업&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ON A.COMPANY_CD&amp;nbsp; &amp;nbsp; &amp;nbsp;= B.COMPANY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; AND A.BU_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; = B.BU_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; AND A.DEMAND_ID&amp;nbsp; &amp;nbsp; &amp;nbsp; = B.DEMAND_ID&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; AND CASE WHEN B.WC_CD = &#39;BOD_OP&#39; THEN REPLACE(B.PROD_ORDER_NO, &#39;BOD&#39;, &#39;&#39;) ELSE B.PROD_ORDER_NO END&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; = SUBSTR(A.PROD_ORDER_NO, 1, LENGTH(CASE WHEN B.WC_CD = &#39;BOD_OP&#39; THEN REPLACE(B.PROD_ORDER_NO, &#39;BOD&#39;, &#39;&#39;) ELSE B.PROD_ORDER_NO END))&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ) A&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; WHERE A.SEQ = 1&lt;/span&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="pdw2pKIptuRdF_-jsix2-110">
          <mxGeometry x="4.9999999999999964" y="30" width="1000" height="560" as="geometry" />
        </mxCell>
        <mxCell id="3frRKVwkdYMzkcdnKzqC-98" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;entryX=0.5;entryY=0;entryDx=0;entryDy=0;" edge="1" parent="1" source="pdw2pKIptuRdF_-jsix2-112" target="pdw2pKIptuRdF_-jsix2-114">
          <mxGeometry relative="1" as="geometry" />
        </mxCell>
        <mxCell id="pdw2pKIptuRdF_-jsix2-112" value="MASTER_PLAN2 테이블&amp;nbsp;--WC별, DEMAND별 WORK LOAD를 고려한 가장 빠른 BUCKET 계산" style="swimlane;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;" vertex="1" parent="1">
          <mxGeometry x="90.44000000000008" y="28250" width="1125" height="600" as="geometry" />
        </mxCell>
        <mxCell id="pdw2pKIptuRdF_-jsix2-113" value="&lt;div&gt;&amp;nbsp;SELECT A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID, A.DEMAND_ORDER_NO, A.LVL, A.LVL_R, A.PLANT_CD, A.WC_CD, A.ORDER_TYPE&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.ITEM_CD, A.ITEM_TYPE, A.PARENT_ITEM_CD, A.CUST_CD, A.PLAN_STATUS, A.DUE_DATE, A.REF_ORDER_NO, A.SETUP_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY, A.TRANSFER_QTY, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.TRANSFER_MIN, A.BOD_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET, A.PRE_PST, A.RUN_MIN, A.BASE_RUN_MIN, A.WORK_MIN, A.PRIORITY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PRE_ACCUM_WC_WORK_MIN, A.ACCUM_WC_WORK_MIN, A.PRE_ACCUM_DMD_WORK_BUCKET, A.ACCUM_DMD_WORK_BUCKET&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; --누적 시간 산출&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.WORK_BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;SUM(A.WORK_MIN)&lt;/font&gt;&lt;/b&gt; OVER (PARTITION BY A.COMPANY_CD, A.BU_CD, A.PLANT_CD, A.WC_CD) AS&amp;nbsp;&amp;nbsp;&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WC_BASE_LOAD &lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;/font&gt;&lt;/b&gt;--WC별 PO의 누적 LOAD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , (SELECT K.AVG_WORK_MIN FROM AVG_WORK K WHERE A.PLANT_CD = K.PLANT_CD AND A.WC_CD = K.WC_CD) AS WC_AVG_WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN A.PLANT_CD = &#39;BOD&#39; THEN &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;1&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE (SELECT&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; CASE WHEN A.PRE_ACCUM_DMD_WORK_BUCKET = 0 OR A.WORK_MIN = 0 THEN &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;1&lt;/font&gt;&lt;/b&gt;&amp;nbsp; --전공정 작업 시간이 없으면 가장 빠른 시간으로 처리&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;NVL(MIN(K.BUCKET_RANK), 1) &lt;/font&gt;&lt;/b&gt;END&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM&amp;nbsp; &amp;nbsp;BUCKET K&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;WHERE&amp;nbsp; A.PLANT_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; = K.PLANT_CD&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND&amp;nbsp; &amp;nbsp; A.WC_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;= K.WC_CD&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND&amp;nbsp; &amp;nbsp; K.ACCUM_AVAIL_WORK_MIN BETWEEN A.PRE_ACCUM_WC_WORK_MIN AND A.PRE_ACCUM_WC_WORK_MIN + 41400) END AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WC_PST_BUCKET&lt;/font&gt;&lt;/b&gt; --WC별 PO의 누적 시작 BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN A.PLANT_CD = &#39;BOD&#39; THEN A.WORK_MIN / (SELECT K.AVG_WORK_MIN FROM AVG_WORK K WHERE A.PLANT_CD = K.PLANT_CD AND A.WC_CD = K.WC_CD)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE (SELECT /*+ use_hash(K) cardinality(K 16)*/&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; CASE WHEN A.PRE_ACCUM_DMD_WORK_BUCKET = 0 OR A.WORK_MIN = 0 THEN 1&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE NVL(MIN(K.BUCKET_RANK), 1) END&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM&amp;nbsp; &amp;nbsp;BUCKET K&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;WHERE&amp;nbsp; A.PLANT_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; = K.PLANT_CD&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND&amp;nbsp; &amp;nbsp; A.WC_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;= K.WC_CD&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND&amp;nbsp; &amp;nbsp; K.ACCUM_AVAIL_WORK_MIN BETWEEN A.ACCUM_WC_WORK_MIN AND A.ACCUM_WC_WORK_MIN + 41400) END AS WC_PET_BUCKET --WC별 PO의 누적 끝 BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , (SELECT /*+ use_hash(K) cardinality(K 16)*/&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; CASE WHEN A.PRE_ACCUM_DMD_WORK_BUCKET = 0 OR A.WORK_MIN = 0 THEN 1&amp;nbsp; --전공정 작업 시간이 없으면 가장 빠른 시간으로 처리&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;ELSE NVL(MIN(K.BUCKET_RANK), 1) END&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM&amp;nbsp; &amp;nbsp;BUCKET K&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;WHERE&amp;nbsp; A.PLANT_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; = K.PLANT_CD&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND&amp;nbsp; &amp;nbsp; A.WC_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;= K.WC_CD&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND&amp;nbsp; &amp;nbsp; K.ACCUM_AVAIL_WORK_MIN BETWEEN A.PRE_ACCUM_WC_WORK_MIN + LEAST(A.TRANSFER_MIN, A.WORK_MIN)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND A.PRE_ACCUM_WC_WORK_MIN + LEAST(A.TRANSFER_MIN, A.WORK_MIN) + 41400)&amp;nbsp; AS TRANS_1ST_BUCKET --첫 이동 BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM MASTER_PLAN1&amp;nbsp; &amp;nbsp; A&lt;/span&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="pdw2pKIptuRdF_-jsix2-112">
