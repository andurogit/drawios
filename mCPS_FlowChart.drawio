        <mxCell id="pdw2pKIptuRdF_-jsix2-106" value="BUCKET테이블" style="swimlane;whiteSpace=wrap;html=1;fillColor=#dae8fc;strokeColor=#6c8ebf;" vertex="1" parent="1">
