        <mxCell id="pdw2pKIptuRdF_-jsix2-103" value="&lt;div&gt;SELECT A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID, A.DEMAND_ORDER_NO, A.PLANT_CD, A.WC_CD, A.ORDER_TYPE&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.ITEM_CD, A.PARENT_ITEM_CD, A.ITEM_TYPE, A.CUST_CD, A.PLAN_STATUS, A.DUE_DATE, A.REF_ORDER_NO, A.SETUP_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY, A.TRANSFER_QTY, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.TRANSFER_MIN, A.BOD * 60 AS BOD_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET, A.LVL, A.LVL_R, A.PRE_PST&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.RUN_TIME &lt;/font&gt;&lt;/b&gt;/ 60&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;RUN_MIN&amp;nbsp;&lt;/font&gt;&lt;/b&gt; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;--품목별 단위 작업 시간&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.RUN_TIME&lt;/font&gt;&lt;/b&gt; / NVL(&lt;b&gt;&lt;font color=&quot;#ff8000&quot;&gt;B.MAX_CAPA / B.MIN_CAPA&lt;/font&gt;&lt;/b&gt;, 1) / 60 AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;BASE_RUN_MIN&lt;/font&gt;&lt;/b&gt;&amp;nbsp; --CAPA를 고려한 품목별 단위 작업 시간&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.WORK_TIME&lt;/font&gt;&lt;/b&gt; / NVL(&lt;b&gt;&lt;font color=&quot;#ff8000&quot;&gt;B.MAX_CAPA / B.MIN_CAPA&lt;/font&gt;&lt;/b&gt;, 1) / 60 AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WORK_MIN&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN A.PLAN_QTY = 0 THEN &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;1&lt;/font&gt;&lt;/b&gt; ELSE &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;2&lt;/font&gt;&lt;/b&gt; END&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||NVL(A.&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;PLAN_STATUS&lt;/font&gt;&lt;/b&gt;, &#39;P&#39;)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||NVL(&lt;b&gt;&lt;font color=&quot;#00cccc&quot;&gt;A.PRE_PST,&lt;/font&gt;&lt;/b&gt; SUBSTR(&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.LPST,&lt;/font&gt;&lt;/b&gt; 1, 6)||&#39;31&#39;)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||CASE WHEN A.EPST = (SELECT START_DATE FROM PLAN_START_DATE) THEN &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;0&lt;/font&gt;&lt;/b&gt; ELSE &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;1&lt;/font&gt;&lt;/b&gt; END&amp;nbsp; --준비된 오더를 먼저 작업하도록 함.&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||SUBSTR(&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.DUE_DATE&lt;/font&gt;&lt;/b&gt;, 1, 6)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||CASE WHEN A.REF_ORDER_NO IS NULL THEN &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;1&lt;/font&gt;&lt;/b&gt; ELSE &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;0&lt;/font&gt;&lt;/b&gt; END&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||DECODE(A.ORDER_TYPE, &#39;PO&#39;, &lt;font color=&quot;#ff0000&quot;&gt;&lt;b&gt;1&lt;/b&gt;,&lt;/font&gt; &#39;IN&#39;, &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;2, 3&lt;/font&gt;&lt;/b&gt;)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.DUE_DATE&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||LPAD(&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;RANK&lt;/font&gt;&lt;/b&gt;() OVER (ORDER BY &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.REQUIRED_QTY&lt;/font&gt;&lt;/b&gt; DESC), 6, &#39;0&#39;)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ||TO_CHAR(LPAD(&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;ROWNUM&lt;/font&gt;&lt;/b&gt;, 6, 0)) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRIORITY_CD&amp;nbsp;&lt;/font&gt;&lt;/b&gt; &amp;nbsp;--GLOBAL 작업 순서&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM BASE_PLAN&amp;nbsp; &amp;nbsp; A&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;LEFT JOIN &lt;b&gt;&lt;font color=&quot;#ff8000&quot;&gt;WORK_CENTER B&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ON&amp;nbsp; A.COMPANY_CD&amp;nbsp; &amp;nbsp; = B.COMPANY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; AND A.BU_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;= B.BU_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; AND A.PLANT_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; = B.PLANT_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; AND A.WC_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;= B.WC_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; ORDER BY PROD_ORDER_NO&lt;/span&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="pdw2pKIptuRdF_-jsix2-102">
          <mxGeometry x="5.4999999999999964" y="30" width="930" height="360" as="geometry" />
        </mxCell>
