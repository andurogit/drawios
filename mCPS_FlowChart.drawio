        <mxCell id="Y5yPyT8mAZHPssHcac92-1246" value="INSERT INTO &lt;b&gt;TB_DYN_WORK_ORDER&lt;/b&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;br&gt;&amp;nbsp; &amp;nbsp; SELECT A.COMPANY_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.BU_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.PLAN_ID&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.PLANT_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.WC_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, NVL(L.PROD_ORDER_NO, D.BUCKET||&#39;_&#39;||A.ITEM_CD) AS WORK_ORDER_NO&amp;nbsp; --L.PROD_ORDER_NO는 신규 테이블의 WORK_ORDER_NO&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.ITEM_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CEIL(SUM(A.PLAN_QTY) OVER (PARTITION BY D.BUCKET, A.ITEM_CD)) AS BKT_PROD_QTY&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CEIL(SUM(A.ASSIGNED_INV_QTY) OVER (PARTITION BY D.BUCKET, A.ITEM_CD)) AS BKT_ASSIGN_INV_QTY&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CEIL(SUM(A.ORDER_QTY) OVER (PARTITION BY D.BUCKET, A.ITEM_CD)) AS BKT_ORDER_QTY&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, MIN(A.ADJ_PST) OVER (PARTITION BY D.BUCKET, A.ITEM_CD) AS WORK_START&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, RANK() OVER (PARTITION BY D.BUCKET, A.ITEM_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ORDER BY A.DUE_DATE, NVL(C.ADJ_PST, &#39;99999999&#39;) ) AS WORK_SEQ&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, MIN(C.ADJ_PST) OVER (PARTITION BY D.BUCKET, A.ITEM_CD) AS NEXT_START&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, ROUND(SUM(A.RUN_TIME) OVER (PARTITION BY SUBSTR(A.ADJ_PST, 1, 6), A.ITEM_CD) / 3600, 2) AS RUN_HOUR&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.PROD_ORDER_NO&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.DEMAND_ID AS DEMAND_ORDER_NO&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, B.DEMAND_ID&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CEIL(A.ORDER_QTY) AS ORDER_QTY&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CEIL(A.ASSIGNED_INV_QTY) AS ASSIGNED_INV_QTY&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CEIL(A.PLAN_QTY) AS PLAN_QTY&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, NVL(C.PLANT_CD, &#39;MNM&#39;) AS TO_PLAN_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, NVL(C.WC_CD, &#39;MNM&#39;) AS TO_WC_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, NVL(A.PARENT_ITEM_CD, A.ITEM_CD) AS PARENT_ITEM_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.DUE_DATE&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.ADJ_PST AS PST&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.ADJ_PET AS PET&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, NVL(C.ADJ_PST, B.DUE_DATE) AS TO_PST&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.PLAN_STATUS&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.FIXED_FLAG&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.RUN_TIME&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.TIME_UOM_CD&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &#39;USP_TB_DYN_WEEKLY_SOP&#39; AS CREATE_ID&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, SYSDATE AS CREATE_DTTM&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, NULL AS UPDATE_ID&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, NULL AS UPDATE_DTTM&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; FROM &lt;b&gt;TB_DYN_MASTER_PLAN &lt;/b&gt;A&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;INNER JOIN (SELECT YYYYMMDD&lt;br&gt;&lt;blockquote style=&quot;margin: 0 0 0 40px; border: none; padding: 0px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ,CASE WHEN (SELECT &lt;b&gt;ATTB_1_CD &lt;/b&gt;FROM TB_MST_CODE&lt;br&gt; &lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; WHERE GROUP_CD = &#39;&lt;b&gt;WORK_ORDER&lt;/b&gt;&#39;&lt;br&gt; &lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt;&#x9;&lt;/span&gt;&amp;nbsp; &amp;nbsp;&amp;nbsp; &amp;nbsp; AND CODE_CD = &#39;&lt;b&gt;BKT&lt;/b&gt;&#39;) = &#39;&lt;b&gt;WEEKS&lt;/b&gt;&#39;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;THEN YEARMONTH_STD||WEEKOFYEAR ELSE YEARMONTH_STD END AS &lt;b&gt;&lt;font color=&quot;#3333ff&quot;&gt;BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/blockquote&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM TB_MST_CALENDAR) D&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ON D.YYYYMMDD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;= A.PST&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp;INNER JOIN TB_DYN_MRP_BASE_PLAN B&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ON&amp;nbsp;B.PLAN_ID&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; = SUBSTR(A.PLAN_ID, 1, 9)||&#39;I01&#39;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND B.PROD_ORDER_NO&amp;nbsp; = A.PROD_ORDER_NO&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; LEFT JOIN TB_DYN_MASTER_PLAN C&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; LEFT JOIN TB_DYN_WORK_ORDER L&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;" style="whiteSpace=wrap;html=1;align=left;" vertex="1" parent="1">
          <mxGeometry x="566.95" y="81765" width="788.5" height="770" as="geometry" />
        </mxCell>
        <mxCell id="Y5yPyT8mAZHPssHcac92-1248" value="&lt;div&gt;UPDATE TB_MST_PLAN_VERSION&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;SET CUT_OFF_FLAG = &#39;Y&#39;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , RELEASE_FLAG = &#39;Y&#39;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp;WHERE PLAN_ID&amp;nbsp; &amp;nbsp; &amp;nbsp; = p_PLAN_ID&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND COMPANY_CD&amp;nbsp; &amp;nbsp;= p_COMPANY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;AND BU_CD&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; = p_BU_CD&lt;/span&gt;&lt;/div&gt;" style="whiteSpace=wrap;html=1;align=left;" vertex="1" parent="1">
