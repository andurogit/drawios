        <mxCell id="pdw2pKIptuRdF_-jsix2-118" value="&lt;div&gt;SELECT &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID&lt;/font&gt;&lt;/b&gt;, &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.DEMAND_ORDER_NO&lt;/font&gt;&lt;/b&gt;,&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt; A.PLANT_CD, A.WC_CD, A.ITEM_CD, A.PARENT_ITEM_CD, A.ITEM_TYPE&lt;/font&gt;&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.LVL, A.LVL_R&lt;/font&gt;&lt;/b&gt;, &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY&lt;/font&gt;&lt;/b&gt;, &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.TRANSFER_QTY&lt;/font&gt;&lt;/b&gt;, &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.WORK_MIN&lt;/font&gt;&lt;/b&gt;, &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.BOD_MIN,&lt;/font&gt;&lt;font color=&quot;#ff00ff&quot;&gt; A.RUN_MIN&lt;/font&gt;&lt;/b&gt;, &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.BASE_RUN_MIN&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;, A.DUE_DATE, A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET,&lt;/font&gt;&lt;font color=&quot;#00cccc&quot;&gt; A.PRE_PST&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;, A.SETUP_TYPE, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.REF_ORDER_NO&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.PRIORITY_CD&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;ROW_NUMBER&lt;/b&gt;() OVER (PARTITION BY A.PLANT_CD, A.WC_CD ORDER BY &lt;b&gt;PRIORITY_CD&lt;/b&gt;) AS &lt;b&gt;WC_RANK&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;0 AS ACCUM_BOD&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.PRE_ACCUM_WC_WORK_MIN &lt;/font&gt;&lt;/b&gt;AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRE_WC_PO_MIN&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.ACCUM_DMD_WORK_BUCKET &lt;/font&gt;&lt;/b&gt;AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRE_DMD_PO_MIN&amp;nbsp;&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.WC_BASE_LOAD&lt;/font&gt;&lt;/b&gt; --WC별 PO의 누적 LOAD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;A.WC_PST_BUCKET&lt;/font&gt;&lt;/b&gt; AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WC_BASE_BUCKET_START&lt;/font&gt;&lt;/b&gt;--WC별 PO의 누적 시작 BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, ROUND(A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;ADJ_PST_BUCKET&lt;/font&gt;&lt;/b&gt;, 0) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;ADJ_PST_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRE_ACCUM_DMD_WORK_BUCKET &lt;/font&gt;&lt;/b&gt;AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRE_DMD_BASE_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX_ACCUM_WORK_BUCKET &lt;/font&gt;&lt;/b&gt;AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;PRE_DMD_MAX_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.DMD_MAX_PET_BUCKET AS DMD_MAX_BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX_DMD_PST_BUCKET_LVL &lt;/font&gt;&lt;/b&gt;AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX_BUCKET_LVL&amp;nbsp; &lt;/font&gt;&lt;/b&gt;--가장 부하가 많이 걸려서 지연되는 BUCKET이 속한 LVL_R값&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CASE WHEN A.ADJ_PST_BUCKET &amp;lt; 1 THEN 1 ELSE ROUND(A.ADJ_PST_BUCKET, 0) END AS ADJ_PST_BUCKET3&amp;nbsp; --공정 중에서 최대 BUCKET 값 (최대 부하 공정), BOD 반영&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;, CASE WHEN A.PRE_ACCUM_DMD_WORK_BUCKET = 0 OR A.WORK_MIN = 0 THEN TO_CHAR(A.EPST)&amp;nbsp; --전공정 작업 시간이 없으면 가장 빠른 시간으로 처리&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ELSE CASE WHEN A.ADJ_PST &amp;lt; (SELECT START_DATE FROM PLAN_START_DATE)&lt;/span&gt;&lt;