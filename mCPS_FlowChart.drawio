        <mxCell id="pdw2pKIptuRdF_-jsix2-115" value="&lt;div&gt;SELECT A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID, A.DEMAND_ORDER_NO, A.LVL, A.LVL_R, A.PLANT_CD, A.WC_CD, A.ORDER_TYPE&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.ITEM_CD, A.ITEM_TYPE, A.PARENT_ITEM_CD, A.CUST_CD, A.PLAN_STATUS, A.DUE_DATE, A.REF_ORDER_NO, A.SETUP_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY, A.TRANSFER_QTY, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.TRANSFER_MIN, A.BOD_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET, A.PRE_PST, A.RUN_MIN, A.BASE_RUN_MIN, A.WORK_MIN, A.PRIORITY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PRE_ACCUM_WC_WORK_MIN, A.ACCUM_WC_WORK_MIN, A.PRE_ACCUM_DMD_WORK_BUCKET, A.ACCUM_DMD_WORK_BUCKET, A.WC_AVG_WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.WC_BASE_LOAD, A.WORK_BUCKET, A.WC_PST_BUCKET, A.WC_PET_BUCKET, A.TRANS_1ST_BUCKET--, A.DMD_PST_BUCKET, A.DMD_PET_BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MAX(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD &lt;/b&gt;= A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;ACCUM_DMD_WORK_BUCKET &lt;/font&gt;&lt;/b&gt;ELSE &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;0&lt;/font&gt;&lt;/b&gt; END) OVER (PARTITION BY A.DEMAND_ID) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX_ACCUM_WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MAX(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD &lt;/b&gt;= A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN A.WC_CD ELSE NULL END) OVER (PARTITION BY A.DEMAND_ID) AS MAX_WC&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MAX(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD &lt;/b&gt;= A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN A.PROD_ORDER_NO ELSE NULL END) OVER (PARTITION BY A.DEMAND_ID) AS MAX_DMD_PST_PROD_ORDER&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MAX(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD &lt;/b&gt;= A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WC_PST_BUCKET &lt;/font&gt;&lt;/b&gt;ELSE &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;0 &lt;/font&gt;&lt;/b&gt;END) OVER (PARTITION BY A.DEMAND_ID) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;DMD_MAX_PST_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MAX(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD &lt;/b&gt;= A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN A.WC_PET_BUCKET ELSE 0 END) OVER (PARTITION BY A.DEMAND_ID) AS DMD_MAX_PET_BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MAX(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD &lt;/b&gt;= A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN A.WC_AVG_WORK_MIN ELSE 0 END) OVER (PARTITION BY A.DEMAND_ID) AS MAX_WC_AVG_WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MAX(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD &lt;/b&gt;= A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN A.&lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;WORK_BUCKET &lt;/font&gt;&lt;/b&gt;ELSE &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;0&lt;/font&gt;&lt;/b&gt; END) OVER (PARTITION BY A.DEMAND_ID) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX_WORK_BUCKET&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , MIN(CASE WHEN A.&lt;b&gt;MAX_DMD_PST_PROD&lt;/b&gt; = A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO THEN &lt;b&gt;&lt;font color=&quot;#ff0000&quot;&gt;A.LVL_R&lt;/font&gt;&lt;/b&gt; ELSE &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;NULL &lt;/font&gt;&lt;/b&gt;END) OVER (PARTITION BY A.DEMAND_ID) AS &lt;b&gt;&lt;font color=&quot;#ff00ff&quot;&gt;MAX_DMD_PST_BUCKET_LVL&lt;/font&gt;&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM (&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;SELECT A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID, A.DEMAND_ORDER_NO, A.LVL, A.LVL_R, A.PLANT_CD, A.WC_CD, A.ORDER_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.ITEM_CD, A.ITEM_TYPE, A.PARENT_ITEM_CD, A.CUST_CD, A.PLAN_STATUS, A.DUE_DATE, A.REF_ORDER_NO, A.SETUP_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY, A.TRANSFER_QTY, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.TRANSFER_MIN, A.BOD_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET, A.PRE_PST, A.RUN_MIN, A.BASE_RUN_MIN, A.WORK_MIN, A.PRIORITY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PRE_ACCUM_WC_WORK_MIN, A.ACCUM_WC_WORK_MIN, A.PRE_ACCUM_DMD_WORK_BUCKET, A.ACCUM_DMD_WORK_BUCKET, A.WC_AVG_WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.WC_BASE_LOAD, A.WORK_BUCKET, A.WC_PST_BUCKET, A.WC_PET_BUCKET, A.TRANS_1ST_BUCKET--, A.DMD_PST_BUCKET, A.DMD_PET_BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , &lt;b&gt;MIN(A.MAX_DMD_PST_PROD) OVER (PARTITION BY A.DEMAND_ID)&lt;/b&gt; AS &lt;b&gt;MAX_DMD_PST_PROD&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM (&amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;SELECT A.COMPANY_CD, A.BU_CD, A.PLAN_ID, A.PROD_ORDER_NO, A.DEMAND_ID, A.DEMAND_ORDER_NO, A.LVL, A.LVL_R, A.PLANT_CD, A.WC_CD, A.ORDER_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.ITEM_CD, A.ITEM_TYPE, A.PARENT_ITEM_CD, A.CUST_CD, A.PLAN_STATUS, A.DUE_DATE, A.REF_ORDER_NO, A.SETUP_TYPE&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PLAN_QTY, A.REQUIRED_QTY, A.ASSIGNED_INV_QTY, A.TRANSFER_QTY, A.MIN_LOT, A.MAX_LOT, A.MULTI_LOT, A.TRANSFER_MIN, A.BOD_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.EPST, A.LPST, A.LPET, A.ORG_EPST, A.ORG_LPST, A.ORG_LPET, A.PRE_PST, A.RUN_MIN, A.BASE_RUN_MIN, A.WORK_MIN, A.PRIORITY_CD&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.PRE_ACCUM_WC_WORK_MIN, A.ACCUM_WC_WORK_MIN, A.PRE_ACCUM_DMD_WORK_BUCKET, A.ACCUM_DMD_WORK_BUCKET, A.WC_AVG_WORK_MIN&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , A.WC_BASE_LOAD, A.WORK_BUCKET, A.WC_PST_BUCKET, A.WC_PET_BUCKET, A.TRANS_1ST_BUCKET--, A.DMD_PST_BUCKET, A.DMD_PET_BUCKET&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; , CASE WHEN MAX(GREATEST(A.WC_PET_BUCKET, A.ACCUM_DMD_WORK_BUCKET)) OVER (PARTITION BY A.DEMAND_ID)&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;= GREATEST(A.WC_PET_BUCKET, A.ACCUM_DMD_WORK_BUCKET)&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;THEN A.LVL_R||&#39;_&#39;||A.PROD_ORDER_NO ELSE &#39;X&#39;||&#39;_&#39;||A.PROD_ORDER_NO END&amp;nbsp; &amp;nbsp; &amp;nbsp; AS &lt;b&gt;MAX_DMD_PST_PROD&lt;/b&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;FROM MASTER_PLAN2&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;A&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ) A&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ) A&lt;/span&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="pdw2pKIptuRdF_-jsix2-114">
